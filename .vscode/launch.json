{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "build (next build debug)",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--inspect",
                "--enable-source-maps"
            ],
            "program": "${workspaceFolder}/node_modules/next/dist/bin/next",
            "args": [
                "build"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "autoAttachChildProcesses": true,
            "sourceMaps": true,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "debug (out) in Chrome",
            "type": "chrome",
            "preLaunchTask": "serve:out",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack://_N_E/./*": "${webRoot}/*",
                "webpack://_N_E/*": "${webRoot}/*",
                "webpack:///./*": "${webRoot}/*",
                "webpack:///*": "${webRoot}/*"
            }
        }
    ]
}